# Development Environment Variables
# Copy this file to .env and fill in your values

# Tycho API Configuration
TYCHO_API_KEY=your_tycho_api_key_here

# Tycho Chain URLs
TYCHO_ETHEREUM_URL=api.tycho.xyz
TYCHO_BASE_URL=api.tycho.xyz
TYCHO_UNICHAIN_URL=api.tycho.xyz

# RPC URLs for each chain
RPC_URL_ETHEREUM=https://eth-mainnet.g.alchemy.com/v2/your_key_here
RPC_URL_BASE=https://base-mainnet.g.alchemy.com/v2/your_key_here
RPC_URL_UNICHAIN=https://unichain.example.com/rpc

# Logging Configuration
RUST_LOG=debug

# Frontend WebSocket URLs (required for development)
VITE_WEBSOCKET_URL=ws://localhost:4001/ws
VITE_WEBSOCKET_URL_ETHEREUM=ws://localhost:4001/ws
VITE_WEBSOCKET_URL_BASE=ws://localhost:4002/ws
VITE_WEBSOCKET_URL_UNICHAIN=ws://localhost:4003/ws